{
    "name": "arkdark",
    "displayName": "ArkDark",
    "description": "Dark theme for VS Code from ArkType",
    "version": "1.0.4",
    "private": true,
    "publisher": "arktypeio",
    "type": "module",
    "scripts": {
        "build": "ts-node --esm ./src/writeTheme.ts",
        "try": "pnpm build && node ./installLocal.js",
        "vspublish": "pnpm build && npx vsce publish"
    },
    "devDependencies": {
        "vsce": "2.15.0",
        "@types/node": "18.15.13",
        "ts-node": "10.9.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/arktypeio/arkdark"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "icon": "icon.png",
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "ArkDark",
                "uiTheme": "vs-dark",
                "path": "./themes/arkDark.json"
            }
        ]
    }
}
