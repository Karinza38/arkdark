{
    "scopeName": "arktype.injection.ts",
    "injectionSelector": "L:source.ts",
    "patterns": [
        {
            "include": "#definition"
        }
    ],
    "repository": {
        "definition": {
            "name": "meta.embedded.arktype.definition",
            "begin": "(.*\\.?(type|scope))\\s*\\(",
            "beginCaptures": {
                "1": {
                    "name": "entity.name.function.ts"
                }
            },
            "end": "(?<=([^\"'`]*([\"'`])[^\"'`]*\\2)*[^\"'`]*)\\)",
            "patterns": [
                {
                    "include": "#expression"
                }
            ]
        },
        "expression": {
            "contentName": "meta.embedded.arktype.expression",
            "begin": "([\"'`])",
            "beginCaptures": {
                "1": {
                    "name": "keyword.operator.ts"
                }
            },
            "end": "(?<!\\\\)(\\1)",
            "endCaptures": {
                "1": {
                    "name": "keyword.operator.ts"
                }
            },
            "patterns": [
                {
                    "include": "#operator"
                },
                {
                    "include": "source.ts#type"
                }
            ]
        },
        "operator": {
            "match": "(<|>|=|%)",
            "captures": {
                "0": {
                    "name": "keyword.operator.ts"
                }
            }
        }
    }
}
